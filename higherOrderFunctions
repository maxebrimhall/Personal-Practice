function iterate(array){
    let newArr = []
     for(let i = 0; i <= array.length; i++){
        return newArr.push(array[i])
     }
     console.log(newArr)
 }
 iterate([1,2,3])


function every(array, test){
    for(let i = 0; i < array.length; i++){
        if(!test(array[i])){
            return false
        }else{
            return true
        }
    }
}